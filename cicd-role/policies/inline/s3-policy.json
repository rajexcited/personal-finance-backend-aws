{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion",
        "s3:GetBucketLocation",
        "s3:BypassGovernanceRetention"
      ],
      "Resource": [
        "arn:aws:s3:::${aws:PrincipalTag/appId}-${aws:PrincipalTag/environment}*",
        "arn:aws:s3:::${aws:PrincipalTag/appId}-${aws:PrincipalTag/environment}*/*"
      ]
    }
  ]
}
